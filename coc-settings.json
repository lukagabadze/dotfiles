{
  "suggest.noselect": false,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "typescript.tsx",
    "javascript.jsx",
    "graphql",
    "css",
    "scss",
    "html",
    "css"
  ],
  "docker.enable": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/docker/compose/master/compose/config/config_schema_v2.0.json": "**/docker-compose*.yml"
  }
}
